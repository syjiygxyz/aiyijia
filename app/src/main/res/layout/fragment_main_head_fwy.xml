<?xml version="1.0" encoding="utf-8"?>
<cn.finalteam.loadingviewfinal.PtrClassicFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android_custom="http://schemas.android.com/tools"
    android:id="@+id/pcf_refresh"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="区域中心顾问"
            android:textColor="@color/black"
            android:paddingLeft="20dp"
            android:gravity="center_vertical"
            android:background="@color/item_divide"/>
        <LinearLayout
            android:id="@+id/ll_qy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"></LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="本站点人员"
            android:textColor="@color/black"
            android:paddingLeft="20dp"
            android:gravity="center_vertical"
            android:background="@color/item_divide"/>
        <LinearLayout
            android:id="@+id/ll_bzd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"></LinearLayout>
    </LinearLayout>
</cn.finalteam.loadingviewfinal.PtrClassicFrameLayout>
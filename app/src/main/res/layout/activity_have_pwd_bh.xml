<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ayj.aiyijia.activity.personcenter.safecenter.HavePwdBhActivity">
    <include layout="@layout/common_title_layout"/>
    <TextView
        android:id="@+id/tv_my_problem1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dip"
        android:textSize="16dip"
        android:textColor="@color/titleLimitToBuy"
        />

    <TextView
        android:id="@+id/tv_my_answer1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dip"
        android:textSize="16dip"
        android:textColor="@color/titleLimitToBuy"
        android:background="@color/white"
        android:layout_marginTop="1dip"
        android:enabled="false"
        />

    <TextView
        android:id="@+id/tv_my_problem2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dip"
        android:textSize="16dip"
        android:textColor="@color/titleLimitToBuy"
        android:background="@color/white"
        android:layout_marginTop="16dip"
        />

    <TextView
        android:id="@+id/tv_my_answer2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dip"
        android:textSize="16dip"
        android:textColor="@color/titleLimitToBuy"
        android:background="@color/white"
        android:layout_marginTop="1dip"
        android:enabled="false"
        />
</LinearLayout>
